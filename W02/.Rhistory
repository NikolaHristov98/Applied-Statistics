rand.Circle<-function(n,x0 = 0,y0 = 0,r){
ro <- runif(n,0,r)
phi <- runif(n,0,2*Pi)
x <- x0+ro*cos(phi)
y <- y0+ro*sin(phi)
return(cbind(x,y))
}
plot(rand.Circle(1000,0,0,1))
